{
  "name": "templates-api-assemblyscript",
  "description": "Web3API Assemblyscript Template",
  "private": true,
  "version": "0.0.1-prealpha.30",
  "scripts": {
    "build": "yarn build:contract && yarn build:web3api",
    "build:web3api": "npx w3 build",
    "build:contract": "node ./scripts/build-contract.js",
    "test:env:up": "npx w3 test-env up",
    "test:env:down": "npx w3 test-env down",
    "deploy": "yarn deploy:contracts && yarn deploy:web3api",
    "deploy:web3api": "npx w3 build --ipfs http://localhost:5001 --test-ens squadprotocol.eth",
    "deploy:contracts": "cd ../eth && PK=0x4f3edf983ac636a65a842ce7c78d9aa706d3b113bce9c46f30d7d21715b23b1d yarn deploy",
    "test": "yarn test:constants && npx w3 query ./recipes/e2e.json",
    "test:constants": "node ./scripts/prep-constants.js"
  },
  "devDependencies": {
    "@web3api/cli": "0.0.1-prealpha.30",
    "@web3api/ethereum-plugin-js": "0.0.1-prealpha.30",
    "@web3api/wasm-as": "0.0.1-prealpha.30",
    "assemblyscript": "0.19.1",
    "ethers": "5.0.7",
    "solc": "0.8.3"
  }
}
