{
  "name": "@squad/subgraph",
  "version": "0.1.0",
  "scripts": {
    "prepare-local": "ts-node config/prepareConfig && mustache config/subgraphConfig.json subgraph.template.yaml > subgraph.yaml",
    "create": "graph create squadgames/squad-POC-subgraph --node https://api.thegraph.com/deploy/",
    "create-local": "graph create squadgames/squad-POC-subgraph --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy squadgames/squad-POC-subgraph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy squadgames/squad-POC-subgraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "test": "yarn prepare-local && mocha -r ts-node/register ./test/*"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.0",
    "@graphprotocol/graph-ts": "^0.20.0",
    "@types/chai": "^4.2.19",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.4",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "ethers": "^5.1.0",
    "mocha": "^9.0.2",
    "mustache": "^4.2.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0"
  }
}
